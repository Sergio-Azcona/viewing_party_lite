<br>
<h3 style="font-family:courier; text-align:center;"><%= button_to "Create a New User", new_user_path, method: :get %></h3>

<br>
<div id="existing_users">
  <h1 style="font-family:courier; text-align:center;">Existing Users</h1>
    <ul style="list-style: none;">
      <% @users.each do |user| %>
        <li><center><h4><%= link_to "#{user.name}", user_path(user) %></h4></center></li>
      <% end %>
    </ul>
</div>